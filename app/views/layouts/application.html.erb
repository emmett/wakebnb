<!DOCTYPE html>
<html>
<head>
	<title>WAKEbnb</title>
	<%= stylesheet_link_tag    'application', media: 'all'%>
	<%= javascript_include_tag 'application' %>
	<%= csrf_meta_tags %>
	<script>$(WAKEbnb.initialize);</script>
</head>
<body>
	<div class="row">
		<%= render partial: "layouts/header" %>
	</div>

	<div class="backdrop">
		<div class="row body">
			<div id="main" class="container">
				<%= yield %>
			</div>
		</div>
	</div>
	

</body>
</html>
